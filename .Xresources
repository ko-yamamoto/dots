! Xterm settings
! Main
XTerm*utf8:               1
XTerm*locale:             true
XTerm*selectToClipboard:  true
XTerm*faceName:           Ricty
XTerm*faceNameDoublesize: Ricty
XTerm*faceSize:           10
XTerm*cjkWidth:           true

!TomorrowNight
#define t_background       #1d1f21
#define t_current_line     #282a2e
#define t_selection        #373b41
#define t_foreground       #c5c8c6
#define base3              #fefbf3
#define black              #2d282a
#define t_commen	   #969896
#define t_red		   #cc6666
#define t_orange	   #de935f
#define t_yellow           #f0c674
#define t_green	           #b5bd68
#define t_aqua	           #8abeb7
#define t_blue	           #81a2be
#define t_purple           #b294bb
#define magenta            #d33682
#define violet             #6c71c4
#define cyan               #2aa198
#define red                #d63334
#define orange             #f79b45
#define yellow             #eab700
#define green              #718c00
#define aqua               #3e999f
#define blue               #4271ae
#define purple             #8959a8
#define white              #ffffff
#define base31             #f6db94


! *.foreground:   t_foreground
! *.background:   t_background
*.foreground:   black
*.background:   base3
*.cursorColor:  #aeafad

! Black / Grey
*.color0:       #000000
*.color8:       #666666

! Red / Bright Red
*.color1:       red
*.color9:       #FF3334

! Green + Bright Green
*.color2:       green
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       orange
*.color11:      t_yellow

! Blue + Bright Blue
*.color4:       blue
*.color12:      blue

! Magenta (Purple) + Bright Magenta
*.color5:       purple
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       aqua
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       base31
*.color15:      t_current_line



XTerm*saveLines:          2000
! XTerm*geometry:           80x25+300+100
XTerm*scrollBar:          true

Emacs*useXIM: false
