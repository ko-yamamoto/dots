alias d. dired .
alias hf helm-find-files $1
alias ff find-file $1
alias ffw find-file-other-exist-window $1

alias less view-file $1
alias view view-file $1

alias cd cd $1;ls
alias ec emacsclient -n $1
alias ll ls -lh $1
alias la ls -alh $1


;; git
alias g git $1

;; git は magit に
alias ms magit-status .
alias ml magit-log
alias mll magit-log-long
Alias mdm magit-diff master
Alias md magit-diff-working-tree
alias mb magit-branch-manager
alias mf magit-fetch-current
alias mr magit-interactive-rebase
